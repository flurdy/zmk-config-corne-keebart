                      display-name = "SYMBOL";
            // ┃                                                 ┃
            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
            // -----------------------------------------------------------------------------------------
            // | BASE | "|" |  #  |  £  |  `  |  "  |       |       |  ^  |      |     |     |  "  |   |
            // |      |  @  |  _  |  :  |  (  |  )  |       |       |     |      |     |     |     |   |
            // |      |  \  |  &  |  $  |  {  |  }  |               |     |      |  ;  |  :  |  ?  |   |
            //                    |     |     |     |               |     |      |     |
            // -----------------------------------------------------------------------------------------
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |   |
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |   |
            // |      |     |     |     |     |     |               |     |      |     |     |     |   |
            //                    |     |     |     |               |     |      |     |
            bindings = <
                &to BASE &kp LS(NUBS) &kp HASH  &kp LS(N3) &kp GRAVE &kp AT   &none  &none &kp CARET &trans &trans   &trans    &kp AT    &none
                &trans   &kp DQT      &kp UNDER &kp COLON  &kp LPAR  &kp RPAR &none  &none &trans    &trans &trans   &trans    &trans    &none
                &trans   &kp NUBS     &kp AMPS  &kp DLLR   &kp LBRC  &kp RBRC              &none     &none  &kp SEMI &kp COLON &kp QMARK &none
                                                &to BASE   &trans    &trans                &trans    &trans &trans
            >;
        };

        navigation_layer {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                                 ┃
                      label = "NAV";
                      display-name = "NAVIGATION";
            // ┃                                                 ┃
