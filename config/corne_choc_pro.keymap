            // -----------------------------------------------------------------------------------------
            // | BASE |  !  |  #  |      |     |     |       |       |     |  7  |  8  |  9  |  0  |   |
            // |      |  +  |  =  | "-"  |  <  |  >  |       |       |  ,  |  4  |  5  |  6  |     |   |
            // |      |  ~  |  %  |  *   |  [  |  ]  |               |  .  |  1  |  2  |  3  |  /  |   |
            //                    | BASE | SPC | TAB |               | ENT |  0  | DEL |
            // -----------------------------------------------------------------------------------------
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |   |
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |   |
            // |      |     |     |     |     |     |               |     |      |     |     |     |   |
            //                    |     |     |     |               |     |      |     |
            bindings = <
                &to BASE &kp EXCL &kp HASH  &none     &none     &none    &none  &none &none     &kp N7 &kp N8 &kp N9 &kp N0   &none
                &trans   &kp PLUS &kp EQUAL &kp MINUS &kp LT    &kp GT   &none  &none &kp COMMA &kp N4 &kp N5 &kp N6 &none    &none
                &trans   &kp PIPE &kp PRCNT &kp STAR  &kp LBKT  &kp RBKT              &kp DOT   &kp N1 &kp N2 &kp N3 &kp FSLH &none
                                            &to BASE  &kp SPACE &kp TAB               &kp RET   &kp N0 &trans
            >;
        };

        symbol_layer {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                                 ┃
