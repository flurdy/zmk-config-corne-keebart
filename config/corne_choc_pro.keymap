#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/rgb.h>

#define BASE 0
#define QWER 1
#define GAME 2
#define NUMB 3
#define SYMB 4
#define NAVI 5
#define FUNC 6
#define MEDI 7
#define CODE 8
#define CSLP 9
#define TILE 10
#define TSIZ 11
#define NORW 12
#define BLUE 13

/ {
    chosen {
        zmk,physical-layout = &default_layout;
    };
    behaviors {
        hml: home_row_mod_left {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            require-prior-idle-ms = <150>;
            tapping-term-ms = <280>;
            quick-tap-ms = <175>;
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <7 8 9 10 11 12 13 20 21 22 23 24 25 26 33 34 35 36 37 38 42 43 44>; // List of keys on the right
            hold-trigger-on-release;
        };
        hmr: home_row_mod_right {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            require-prior-idle-ms = <150>;
            tapping-term-ms = <280>;
            quick-tap-ms = <175>;
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <0 1 2 3 4 5 6 14 15 16 17 18 19 27 28 29 30 31 32 39 40 41>; // List of keys on the left
            hold-trigger-on-release;
        };
    };
};

            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                 CORNE CHOC PRO KEYMAP CONFIGURATION                             ┃
            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                                 ┃
                      label = "BASE";
                      display-name = "Colemak";
            // ┃                                                 ┃
            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
            // --------------------------------------------------------------------------------
            // |      |  Q  |  W  |  E  |  R  |  T  |       |       |  Y  |  U   |  I  |  O  |  P  |  |
            // | BSPC |  A  |  S  |  D  |  F  |  G  |       |       |  H  |  J   |  K  |  L  |  ;  |  |
            // | SHFT |  Z  |  X  |  C  |  V  |  B  |               |  N  |  M   |  ,  |  .  |  /  |  |
            //                    | ESC | SPC | TAB |               | ENT | BSPC | DEL |
            // --------------------------------------------------------------------------------
            // |      | RGUI | RALT | RSHFT | RCTRL | BLUE |       |       |      | RCTRL | RSHFT | RALT | RGUI |  |
            // |      | GUI  | ALT  | SHFT  | CTRL  | TILE |       |       | CODE | CTRL  | SHFT  | ALT  | GUI  |  |
            // |      |      | COMP |       | CODE  | NORW |               |      | FUNC  |       | COMP | MEDI |  |
            //                      |       |       |      |               |      |       |       |
            bindings = <
                &none     &mt RGUI Q  &mt RALT W     &mt RSHFT E &mt RCTRL R    &lt BLUE T   &none   &none &kp Y        &mt RCTRL U   &mt RSHFT I &mt RALT O       &mt RGUI P    &none
                &kp BSPC  &mt LGUI A  &mt LALT S     &mt LSHFT D &mt LCTRL F    &lt TILE G   &none   &none &lt CODE H   &mt LCTRL J   &mt LSHFT K &mt LALT L       &mt LGUI SEMI &none
                &kp LSHFT &mt LSHFT Z &mt LS(RALT) X &kp C       &lt CODE V     &lt NORW B                 &kp N        &lt FUNC M    &kp COMMA   &mt LS(RALT) DOT &lt MEDI FSLH &none
                                                     &kp ESC     &lt NUMB SPACE &lt NAVI TAB               &lt SYMB RET &lt NUMB BSPC &kp DEL
            >;
            sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp PG_UP PG_DN &inc_dec_kp C_PREV C_NEXT &inc_dec_kp C_BRI_DN C_BRI_UP>;
        };

        qwerty_layer {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                                 ┃
                      label = "QWER";
                      display-name = "QWERTY";
            // ┃                                                 ┃
            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
            // -----------------------------------------------------------------------------------------
            // | BASE |  Q  |  W  |  F  |  P  |  B   |       |       |  J  |  L   |  U  |  Y  |  ;  |      |
            // | TAB  |  A  |  R  |  S  |  T  |  G   |       |       |  M  |  N   |  E  |  I  |  O  |      |
            // | SHFT |  Z  |  X  |  C  |  D  |  V   |               |  K  |  H   |  ,  |  .  |  /  |      |
            //                    | ESC | SPC | CTRL |               | ENT | BSPC | DEL |
            // --------------------------------------------------------------------------------
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |      |
            // |      |     |     |     |     |     |       |       |     |      |     |     |     |      |
            // |      |     |     |     |     |     |               |     |      |     |     |     |      |
            //                    |     |     |     |               |     |      |     |
            bindings = <
                &to BASE &kp Q &kp W &kp F &kp P    &kp B &trans &trans &kp J &kp L &kp U    &kp Y    &kp SEMI &trans
                &kp TAB  &kp A &kp R &kp S &kp T    &kp G &trans &trans &kp M &kp N &kp E    &kp I    &kp O    &trans
                &trans   &kp Z &kp X &kp C &kp D    &kp V               &kp K &kp H &kp COMMA &kp DOT &kp FSLH &trans
                                   &trans &kp SPACE &kp LCTRL        &kp RET &trans &trans
            >;
            sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp PG_UP PG_DN &inc_dec_kp C_PREV C_NEXT &inc_dec_kp C_BRI_DN C_BRI_UP>;
        };


        game_layer {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
            // ┃                                                 ┃
